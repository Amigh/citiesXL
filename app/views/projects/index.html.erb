<table class="ui very basic collapsing celled table">
  <thead>
    <tr>
      <th>Population</th>
      <th>Cash flow</th>
      <th>Income</th>
      <th>Money</th>
      <th>Satis people 1</th>
      <th>Satis work 1</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @projects.each do |project| %>
      <tr>
        <td><%= project.population %></td>
        <td><%= project.cash_flow %></td>
        <td><%= project.income %></td>
        <td><%= project.money %></td>
        <td><%= project.satis_people_1 %></td>
        <td><%= project.satis_work_1 %></td>
        <td><%= link_to 'Show', student_project_path(project.student,project) %></td>
        <td><%= link_to 'Edit', edit_student_project_path(project.student,project) %></td>
        <td><%= link_to 'Destroy', student_project_path(project.student,project), method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Project', new_student_project_path %>
<%= link_to 'back', student_path(@student) %>
