<div class=" ui  card" style="margin: 0 auto">
  <div class="ui content">
    <div class="ui header">
      <%= link_to tutorial.title ,tutorial_path(tutorial)%>
    </div>
  </div>
  <div class="content" style="direction: rtl;text-align:right;">
    <div class="ui meta">
      <%= tutorial.description%>
    </div>
  </div>
  <div class="ui extra content">
    <%= tutorial.created_at.to_jalali.strftime("%A %d %B %Y")  %>
  </div>
</div>
